{
  "name": "{{projectName}}",
  "description": "{{description}}",
  "version": "{{version}}",
  "private": true,
  "scripts": {
    "dev": "npm run dev:amp & NODE_ENV=development PORT=8000 node server",
    "build": "npm run build:amp && next build",
    "start": "NODE_ENV=production node server",
    "dev:amp": "webpack --env.NODE_ENV=development --config webpack.amp.config.js",
    "build:amp": "webpack --env.NODE_ENV=production --config webpack.amp.config.js",
    "postinstall": "link-module-alias",
    "preinstall": "command -v link-module-alias && link-module-alias clean || true"
  },
  "_moduleAliases": {
    "react": "node_modules/preact/compat",
    "react-dom": "node_modules/preact/compat",
    "react-ssr-prepass": "node_modules/preact-ssr-prepass"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "css-loader": "^3.4.2",
    "express": "^4.17.3",
    "link-module-alias": "^1.2.0",
    "next": "^9.3.1",
    "next-i18next": "^4.2.1",
    "preact": "^10.3.4",
    "preact-render-to-string": "^5.1.4",
    "preact-ssr-prepass": "^1.0.1",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "require-context": "^1.1.0",
    "style-loader": "^1.1.3"
  },
  "devDependencies": {
    "@types/node": "^13.9.0",
    "@types/react": "^16.9.23",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "@zeit/next-css": "latest",
    "babel-plugin-import": "^1.13.0",
    "clean-webpack-plugin": "^3.0.0",
    "customize-cra": "^0.9.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "lint-staged": "^10.0.8",
    "next-compose-plugins": "^2.2.0",
    "next-transpile-modules": "^3.1.0",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "react-amphtml": "^4.0.2",
    "sass-loader": "^8.0.2",
    "sass-resources-loader": "^2.0.1",
    "styled-jsx-plugin-sass": "^1.0.0",
    "typescript": "^3.8.3",
    "url-loader": "^3.0.0",
    "webpack-cli": "^3.3.11"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --fix"
  }
}
